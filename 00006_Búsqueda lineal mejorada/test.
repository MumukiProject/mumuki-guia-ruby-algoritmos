it "el elemento 8 no existe en la lista []"  do
  expect(existe_elemento?(8, [])).to be_falsy
end

it "el elemento 87 existe en la lista [87]"  do
  expect(existe_elemento?(87, [Numero.new(87)])).to be_truthy
end

it "el elemento 1 existe en la lista [1,2,3]"  do
  expect(existe_elemento?(1, [Numero.new(1), Incomparable])).to be_truthy
end

it "el elemento 8 existe en la lista [1,2,3, 8, 9, 10]"  do
  expect(existe_elemento?(8, [Numero.new(1), Numero.new(2), Numero.new(3), Numero.new(8), Incomparable])).to be_truthy
end

it "el elemento 10 existe en la lista [1,2,3, 8, 9, 10]"  do
  expect(existe_elemento?(10, [Numero.new(1), Numero.new(2), Numero.new(3), Numero.new(8), Numero.new(9), Numero.new(10)])).to be_truthy
end

it "el elemento 6 no existe en la lista []"  do
  expect(existe_elemento?(6, [])).to be_falsy
end

it "el elemento 4 no existe en la lista [1, 2, 3]"  do
  expect(existe_elemento?(4, [Numero.new(1), Numero.new(2), Numero.new(3)])).to be_falsy
end

it "el elemento 7 no existe en la lista [1,2,3, 8, 9, 10]"  do
  expect(existe_elemento?(4, [Numero.new(1), Numero.new(2), Numero.new(3), Numero.new(8), Numero.new(9), Numero.new(10)])).to be_falsy
end
