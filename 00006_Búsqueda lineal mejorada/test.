it "existe_elemento? debe responder true si el elemento es parte de la lista"  do
  expect(existe_elemento?(2, [Numero.new(1), Numero.new(2), Incomparable])).to be_truthy
end

it "existe_elemento? debe responder false si el elemento no es parte de la lista"  do
  expect(existe_elemento?(4, [Numero.new(1), Numero.new(2), Numero.new(3)]).to be_falsy
end
